<!--component/button/icon_button/icon_button.wxml-->
<view class='component cus' catchtap='onTap' style='width: {{ width }}rpx; height: {{ height }}rpx'>
  <image class='icon' src='{{ mSrc }}' style='width: {{ iconWidth }}rpx; height: {{ iconHeight }}rpx; margin-bottom: {{ spacing }}rpx'></image>
  <text class='text' style='font-size: {{ fontSize }}rpx; color: {{ fontColor }}'>{{ mText }}</text>
</view>
