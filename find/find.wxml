<!--find/find.wxml-->
<view data-id='item.id' wx:for='{{ list }}'>
  <e-base-card
    src='{{ item.img }}'
    title='{{ item.title }}'
    titleSize='32'>
    <view slot='sub' class='sub'>
      <e-enhance-text
        icon-cus='iconfont icon-chakan'
        titleFontSize='26'
        title='{{ item.watch }}'>
      </e-enhance-text>
      <text style='color: #999; font-size: 24rpx;'>{{ item.time }}</text>
    </view>
  </e-base-card>
</view>

